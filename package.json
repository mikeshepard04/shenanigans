{
  "name": "traveling-trotter",
  "version": "1.0.0",
  "scripts": {
    "build:ssr": "npm run build:server-stuff && npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles": "ng build --prod && ng build --prod --project traveling-trotter --output-hashing=none",
    "build:server-stuff": "ng run traveling-trotter:server",
    "webpack:server": "webpack --mode development --config webpack.server.config.js --progress --colors"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.15",
    "@angular/common": "~7.2.15",
    "@angular/compiler": "~7.2.15",
    "@angular/core": "~7.2.15",
    "@angular/forms": "~7.2.15",
    "@angular/http": "~7.2.15",
    "@angular/platform-browser": "~7.2.15",
    "@angular/platform-browser-dynamic": "~7.2.15",
    "@angular/platform-server": "^7.2.15",
    "@angular/pwa": "^0.13.8",
    "@angular/router": "~7.2.15",
    "@angular/service-worker": "~7.2.15",
    "@fortawesome/fontawesome-free": "^5.8.2",
    "@ng-bootstrap/ng-bootstrap": "^4.1.2",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.1",
    "bootstrap": "^4.3.1",
    "core-js": "^3.0.1",
    "ngx-markdown": "^7.1.5",
    "rxjs": "~6.5.1",
    "ts-loader": "^5.4.3",
    "zone.js": "~0.9.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular/cli": "~7.3.9",
    "@angular/compiler-cli": "~7.2.15",
    "@angular/language-service": "~7.2.15",
    "@types/jasmine": "~3.3.12",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~11.13.7",
    "codelyzer": "~5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.2",
    "ts-node": "~8.1.0",
    "tslint": "~5.16.0",
