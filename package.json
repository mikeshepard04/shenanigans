{
  "name": "traveling-trotter",
  "version": "1.0.0",
  "scripts": {
    "build:ssr": "npm run build:server-stuff && npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles": "ng build --prod && ng build --prod --project traveling-trotter --output-hashing=none",
    "build:server-stuff": "ng run traveling-trotter:server",
    "webpack:server": "webpack --mode development --config webpack.server.config.js --progress --colors"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.5",
    "@angular/common": "8.2.5",
    "@angular/compiler": "8.2.5",
    "@angular/core": "8.2.5",
    "@angular/forms": "8.2.5",
    "@angular/platform-browser": "8.2.5",
    "@angular/platform-browser-dynamic": "8.2.5",
    "@angular/platform-server": "^8.2.5",
    "@angular/pwa": "^0.803.3",
    "@angular/router": "8.2.5",
    "@angular/service-worker": "8.2.5",
    "@fortawesome/fontawesome-free": "^5.10.2",
    "@ng-bootstrap/ng-bootstrap": "^5.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^8.1.1",
    "bootstrap": "^4.3.1",
    "core-js": "^3.2.1",
    "ngx-markdown": "^8.1.0",
    "rxjs": "~6.5.3",
    "ts-loader": "^6.0.4",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.3",
    "@angular/cli": "8.3.3",
    "@angular/compiler-cli": "8.2.5",
    "@angular/language-service": "8.2.5",
    "@types/jasmine": "~3.4.0",
    "@types/node": "~12.7.4",
    "codelyzer": "~5.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-jasmine": "~2.0.1",
    "protractor": "~5.4.2",
    "ts-node": "~8.3.0",
    "tslint": "~5.19.0",
    "typescript": "3.5.3",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7"
  }
}
