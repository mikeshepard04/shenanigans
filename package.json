{
  "name": "traveling-trotter",
  "version": "1.0.0",
  "scripts": {
    "build:ssr": "npm run build:server-stuff && npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles": "ng build --prod && ng build --prod --project traveling-trotter --output-hashing=none",
    "build:server-stuff": "ng run traveling-trotter:server",
    "webpack:server": "webpack --mode development --config webpack.server.config.js --progress --colors"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.5",
    "@angular/common": "~7.2.5",
    "@angular/compiler": "~7.2.5",
    "@angular/core": "~7.2.5",
    "@angular/forms": "~7.2.5",
    "@angular/http": "~7.2.5",
    "@angular/platform-browser": "~7.2.5",
    "@angular/platform-browser-dynamic": "~7.2.5",
    "@angular/platform-server": "^7.2.5",
    "@angular/router": "~7.2.5",
    "@fortawesome/fontawesome-free": "^5.7.2",
    "@ng-bootstrap/ng-bootstrap": "^4.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^7.0.2",
    "bootstrap": "^4.2.1",
    "core-js": "^2.6.2",
    "ngx-markdown": "^7.1.1",
    "rxjs": "~6.3.3",
    "ts-loader": "^5.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.2",
    "@angular/cli": "~7.3.2",
    "@angular/compiler-cli": "~7.2.5",
    "@angular/language-service": "~7.2.5",
    "@types/jasmine": "~3.3.9",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.12.18",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.12.1",
    "typescript": "~3.2.4",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1"
